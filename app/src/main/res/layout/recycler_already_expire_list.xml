<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <net.nicbell.materiallists.ListItem
        android:id="@+id/alreadyExpireItemElement"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:dividerTint="@color/green"
        app:headline="Zanahorias • 2 kg"
        app:sizeType="TWO_LINES"
        app:supportText="Venció el 10/11/2024">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/alreadyExpireItemLeadingIcon"
            style="@style/MaterialLists.LeadingIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/baseline_close_24"
            app:tint="@color/red" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/alreadyExpireItemTrailingIcon"
            style="@style/MaterialLists.TrailingIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/baseline_delete_24"
            app:tint="@color/red" />

    </net.nicbell.materiallists.ListItem>

</androidx.appcompat.widget.LinearLayoutCompat>